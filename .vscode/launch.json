    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Tyrannoport/bin/Debug/net5.0/Tyrannoport.dll",
            "args": [
                "${workspaceFolder}/test/Tyrannoport.Tests/fixture_data/FailsAndSkipped.trx",
                "${workspaceFolder}/test/Tyrannoport.Tests/fixture_data/SimpleExample.trx"
            ],
            "cwd": "${workspaceFolder}/src/Tyrannoport",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}